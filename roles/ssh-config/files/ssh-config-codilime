### DO NOT EDIT THIS FILE DIRECTLY!
### Rather, edit appropriate file in ansible deployment.

# INTERNET

Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_codilime_5260631d

Host bitbucket.org
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/id_rsa_codilime_5260631d

Host github-personal
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa_personal_2c6819cb

Host bitbucket-personal
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/id_rsa_personal_2c6819cb



# CODILIME INTERNAL

Host gerrit.codilime.com
    HostName gerrit.codilime.com
    User jwarchol
    IdentityFile ~/.ssh/id_rsa_codilime_5260631d



# NTTi3

# idk what this one is for...
Host bitbucket.org-esi
    HostName bitbucket.org
    User git
    IdentityFile ~/.ssh/id_rsa_esi

Host superslave
    HostName superslave
    User root
    IdentityFile ~/.ssh/id_rsa_codilime_5260631d



# LOCAL (VMs etc.)

# I have no idea why, but I cannot ssh to localhost if I don't specify key...
Host jw-cl,localhost
    IdentityFile ~/.ssh/id_rsa_codilime_5260631d

# a trick to get passwordless switch to another user
Host janek
    User janek
    HostName jwlap
    IdentityFile ~/.ssh/id_rsa_personal_2c6819cb

# Fingerprints of vagrant machines change all the time and it's a nuisance
# to be warned about it.  So, turn off the check, but only when connecting
# to user vagrant on localhost or an address in private space.
Match user vagrant host localhost,192.168.*
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    IdentityFile ~/.ssh/custom_vagrant_key

